    "def square(x):\n",
    "    return x * x\n",
    "\n",
    "def exp_improved2(b, n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    elif n % 2 == 0:\n",
    "        # 注意用floor divison\n",
    "        return square(exp_improved(b, n // 2))\n",
    "\n",
    "    else:\n",
    "        return b * exp_improved(b, n - 1)"
   ]
